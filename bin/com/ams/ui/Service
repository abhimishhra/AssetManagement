

class AuthenticationServiceImpl{
	public String verify(String username, String password){
		String result = verifyDao(username , password);
		return result;
	}
}



class AdminServiceImpl{
	public ArrayList assetAvailability(){
		ArrayList list = assetAvailabilityDao();
		return list;
	}
	
	public void addAsset(Asset a){
		boolean result = addAssetDao(a);
		return result;
	}
	
	public ArrayList checkRequests(){
		ArrayList list = checkRequestsDao();
		return list;
	}
	
	public String requestResponce(int requestId){
		String result = requestResponce(requestId);
		return result;
	}
	
	
	
}

class UserServiceImpl{
	public int raiseRequest(AssetForm form){
		int assetRequestId = update(form);
		return assetRequestId;
	 
	}
	
	public String checkStatus(int assetRequestId)
	{
		String result = checkStatusDao(assetRequestId);
		return result;
		
	}
	
}